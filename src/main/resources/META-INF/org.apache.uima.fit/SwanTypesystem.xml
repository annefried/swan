<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>DiscAnnoTypesystem</name>
    <description/>
    <version>1.0</version>
    <vendor/>
  <types>
   <typeDescription>
      <name>de.unisaarland.swan.export.uimaTypes.SwanAnnotation</name>
      <description>Annotation of one annotator.</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>SpanType</name>
          <description>SpanType of the annotation</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>AnnotatorId</name>
          <description>ID of the annotator who created this annotation.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>Labels</name>
          <description>Set of labels assigned to this annotation by the annotator.</description>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
        <elementType>de.unisaarland.swan.export.uimaTypes.SwanLabel</elementType>
        </featureDescription>
      <featureDescription>
          <name>Links</name>
          <description>Links starting at this SwanAnnotation.</description>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>de.unisaarland.swan.export.uimaTypes.SwanLink</elementType>
        </featureDescription>
        <featureDescription>
          <name>AnnotationId</name>
          <description>Id of the annotation (in database, used for assigning links between annotations).</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>de.unisaarland.swan.export.uimaTypes.SwanLabel</name>
      <description>defines a label: name of label + name of LabelSet to which the labels belongs</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>Name</name>
          <description>name of label</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>LabelSet</name>
          <description>LabelSet to which the label belongs.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>de.unisaarland.swan.export.uimaTypes.SwanLink</name>
      <description>Link between two annotations.</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>linkBegin</name>
          <description>SwanAnnotation at which the link starts.</description>
          <rangeTypeName>de.unisaarland.swan.export.uimaTypes.SwanAnnotation</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>linkEnd</name>
          <description>SwanAnnotation at which the link ends.</description>
          <rangeTypeName>de.unisaarland.swan.export.uimaTypes.SwanAnnotation</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>Labels</name>
          <description>labels assigned to the link.</description>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>de.unisaarland.swan.export.uimaTypes.SwanLabel</elementType>
        </featureDescription>
        <featureDescription>
          <name>AnnotatorId</name>
          <description>Id of annotator who created this link.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
